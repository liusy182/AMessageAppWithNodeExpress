<div id='pager'>
	<% if (page.count > 1) { %>
		<% if (page.number) { %>
			<a id='prev' href='/viewpost?page=<%= page.number %>' >Prev</a>
		<% } %>
		<% if (page.number < page.count - 1) { %>
			<% if (page.number) { %>
				&nbsp;&nbsp
			<% } %>
			<a id='next' href='/viewpost?page=<%= page.number + 2 %>' >Next</a>
		<% } %>
	<% } %>
</div>